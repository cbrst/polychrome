(defun color (key) 
  "<%= name %> colorscheme generated by polychrome
   Author: <%= author %>"
  (let ((colors '(:foreground   #x<%= foreground.hex.substr(1) %>
                  :background-0 #x<%= background[0].hex.substr(1) %>
                  :background-1 #x<%= background[1].hex.substr(1) %>
                  :background-2 #x<%= background[2].hex.substr(1) %>
                  :background-3 #x<%= background[3].hex.substr(1) %>
                  :black        #x<%= color[0].hex.substr(1) %>
                  :red          #x<%= color[1].hex.substr(1) %>
                  :green        #x<%= color[2].hex.substr(1) %>
                  :yellow       #x<%= color[3].hex.substr(1) %>
                  :blue         #x<%= color[4].hex.substr(1) %>
                  :magenta      #x<%= color[5].hex.substr(1) %>
                  :cyan         #x<%= color[6].hex.substr(1) %>
                  :white        #x<%= color[7].hex.substr(1) %>
                  :black-1      #x<%= color[8].hex.substr(1) %>
                  :red-1        #x<%= color[9].hex.substr(1) %>
                  :green-1      #x<%= color[10].hex.substr(1) %>
                  :yellow-1     #x<%= color[11].hex.substr(1) %>
                  :blue-1       #x<%= color[12].hex.substr(1) %>
                  :magenta-1    #x<%= color[13].hex.substr(1) %>
                  :cyan-1       #x<%= color[14].hex.substr(1) %>
                  :white-1      #x<%= color[15].hex.substr(1) %>)))
    (getf colors key)))
